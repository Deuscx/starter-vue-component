{
  "name": "library",
  "type": "module",
  "version": "0.0.2-beta.1",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "description": "A simple starter template to build component library for vue3",
  "author": "deuscx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deuscx/starter-vue-component"
  },
  "bugs": "https://github.com/deuscx/starter-vue-component/issues",
  "keywords": [
    "starter",
    "vue3",
    "component",
    "ui"
  ],
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build",
    "play": "pnpm --filter playground dev",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "release": "bumpp -r --excute=\"pnpm i\"",
    "publish": "pnpm -r publish --access public --no-git-checks",
    "postinstall": "npx simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@vueuse/core": "^14.2.1",
    "vue": "^3.5.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^22.19.11",
    "@unocss/eslint-config": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
