{
  "name": "@deuscx/starter-vue-component",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.8",
  "description": "A simple starter template to build component library for vue3",
  "author": "deuscx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deuscx/starter-vue-component"
  },
  "bugs": "https://github.com/deuscx/starter-vue-component/issues",
  "keywords": [
    "starter",
    "vue3",
    "component",
    "ui"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    },
    "./dist/style.css": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite ",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/node": "^20.2.5",
    "@unocss/eslint-config": "^0.53.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/test-utils": "^2.3.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
